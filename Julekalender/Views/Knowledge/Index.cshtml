@model IEnumerable<Julekalender.Models.Knowledge>
@{
    ViewData["Title"] = "Index";
}

<h1>Jule Kalender</h1>

@{
    foreach (var item in Model)
    {
        <div class="day">
            <label/>
            <input type="checkbox" />
            <div class="door">
                <div class="front">1</div>
                <div class="back"></div>
            </div>
        </div>
    }
}



